    "lang": "en",
    "primary_category": "Commentary",
    "ref": "Rashi on Bekhorot 25a:6:2",
    "url": "Rashi_on_Bekhorot.25a.6.2"
  },
  "Rashi on Bekhorot 25a:6:3": {
    "en": "",
