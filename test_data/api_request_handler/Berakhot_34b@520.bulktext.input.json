    "lang": "en",
    "primary_category": "Commentary",
    "ref": "Rashi on Berakhot 34b:14:1",
    "url": "Rashi_on_Berakhot.34b.14.1"
  },
  "Rashi on Berakhot 34b:17:1": {
    "en": "",
